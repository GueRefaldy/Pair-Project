<form action="/admins/vaccine/edit/<%= data.id %>" method="post">

    <div>
        <label for="name">Name</label>
        <input type="text" id="name" name="name" value="<%= data.name %>" readonly>
    </div>
    <div>
        <label for="stock">Stock</label>
        <input type="number" id="stock" name="stock" value="<%= data.stock %>">
    </div>
    <div>
        <label for="category">Category</label>
        <input type="text" id="category" name="category" value="<%= data.category %>" readonly>
    </div>
    
    <button>submit</button>
    </form>